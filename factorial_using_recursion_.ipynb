      "source": [
        "def factorial(x):\n",
        "     if x == 1:\n",
        "        return 1\n",
        "     else:\n",
        "        return (x * factorial(x-1))\n",
        "\n",
        "\n",
        "num = int(input(\"enter a no:\"))\n",
        "print(\"The factorial of\", num, \"is\", factorial(num))"
      ],
