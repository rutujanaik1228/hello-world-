      "source": [
        "def divisors(n):\n",
        "    divisor_list = [1]\n",
        "    for i in range(2,n // 2 + 1):\n",
        "        if n%i== 0:\n",
        "          divisor_list.append(i)\n",
        "    print(\"the list of divisors of\",n,\":\")\n",
        "    return divisor_list\n",
        "a=int(input(\"enter a number:\"))\n",
        "divisors(a)"
      ],
